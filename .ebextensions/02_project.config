option_settings:
  "aws:elasticbeanstalk:application":
    "Application Healthcheck URL": "/status"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: /opt/python/current/app/application.py
    NumProcesses: 3
    NumThreads: 2

container_commands:
  00_shellscript:
    command: "bash install.sh"
  01_packages:
    command: "pip install -r requirements.txt"
